{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "cli": {
        "packageManager": "npm",
        "analytics": "fd065fd0-4302-4012-8a79-f894134f2eb9"
    },
    "newProjectRoot": "projects",
    "projects": {
        "partner-app": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss",
                    "skipTests": true
                },
                "@schematics/angular:class": {
                    "skipTests": true
                },
                "@schematics/angular:directive": {
                    "skipTests": true
                },
                "@schematics/angular:guard": {
                    "skipTests": true
                },
                "@schematics/angular:interceptor": {
                    "skipTests": true
                },
                "@schematics/angular:pipe": {
                    "skipTests": true
                },
                "@schematics/angular:resolver": {
                    "skipTests": true
                },
                "@schematics/angular:service": {
                    "skipTests": true
                }
            },
            "root": "projects/partner-app",
            "sourceRoot": "projects/partner-app/src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:application",
                    "options": {
                        "outputPath": "dist/partner-app",
                        "index": "projects/partner-app/src/index.html",
                        "browser": "projects/partner-app/src/main.ts",
                        "polyfills": [
                            "zone.js"
                        ],
                        "tsConfig": "projects/partner-app/tsconfig.app.json",
                        "inlineStyleLanguage": "scss",
                        "stylePreprocessorOptions": {
                            "includePaths": [
                                "projects/partner-app/src/styles"
                            ]
                        },
                        "assets": [
                            {
                                "glob": "**/*",
                                "input": "projects/partner-app/public"
                            }
                        ],
                        "styles": [
                            "projects/partner-app/src/styles.scss"
                        ],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "500kB",
                                    "maximumError": "1MB"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "4kB",
                                    "maximumError": "8kB"
                                }
                            ],
                            "outputHashing": "all"
                        },
                        "development": {
                            "optimization": false,
                            "extractLicenses": false,
                            "sourceMap": true
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "port": 4200
                    },
                    "configurations": {
                        "production": {
                            "buildTarget": "partner-app:build:production"
                        },
                        "development": {
                            "buildTarget": "partner-app:build:development"
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n"
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "polyfills": [
                            "zone.js",
                            "zone.js/testing"
                        ],
                        "tsConfig": "projects/partner-app/tsconfig.spec.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            {
                                "glob": "**/*",
                                "input": "projects/partner-app/public"
                            }
                        ],
                        "styles": [
                            "projects/partner-app/src/styles.scss"
                        ],
                        "scripts": []
                    }
                }
            }
        },
